    "ServerlessDashapiDashsampleLambdaVersiongjDyhDsiSP1evBM2awnOiPPTA62Lk7d3z9Ex6uxWEbo": {
      "Type": "AWS::Lambda::Version",
      "DeletionPolicy": "Retain",
      "Properties": {
        "FunctionName": {
          "Ref": "ServerlessDashapiDashsampleLambdaFunction"
